@model IEnumerable<ContosoUniversity.Models.Parking>
@{
    ViewBag.Title = "Index";
}

<h2>ManageParking</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ParkingID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OccupantID)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ParkingID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OccupantID)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ParkingID }) |
                @Html.ActionLink("Details", "Details", new { id = item.ParkingID }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ParkingID })
            </td>
        </tr>
    }

</table>
